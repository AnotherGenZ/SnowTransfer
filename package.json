{
  "name": "snowtransfer",
  "version": "0.7.2",
  "description": "Minimalistic Rest client for the Discord Api",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=12.0.0"
  },
  "keywords": [
    "discord",
    "api",
    "rest",
    "http",
    "microservice",
    "discord bot",
    "discord bot microservice",
    "microservice bot"
  ],
  "scripts": {
    "build:src": "tsup src/index.ts --dts --sourcemap --format cjs --target node12 --minify",
    "build:docs": "typedoc --name SnowTransfer --excludeExternals --sort static-first --sort alphabetical"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DasWolke/SnowTransfer.git"
  },
  "bugs": "https://github.com/DasWolke/SnowTransfer/issues",
  "author": "wolke <wolke@weeb.sh>",
  "license": "MIT",
  "dependencies": {
    "centra": "^2.6.0",
    "discord-api-types": "^0.37.46",
    "form-data": "^4.0.0"
  },
  "devDependencies": {
    "@types/centra": "^2.2.0",
    "@types/node": "^20.3.2",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.43.0",
    "tsup": "^7.1.0",
    "typedoc": "^0.24.8",
    "typedoc-plugin-mdn-links": "^3.0.3",
    "typescript": "^5.1.6"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ]
}
