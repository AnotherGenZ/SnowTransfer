{
  "name": "snowtransfer",
  "version": "0.8.4",
  "description": "Minimalistic Rest client for the Discord Api",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=12.0.0"
  },
  "keywords": [
    "discord",
    "api",
    "rest",
    "http",
    "microservice",
    "discord bot",
    "discord bot microservice",
    "microservice bot"
  ],
  "scripts": {
    "build:src": "tsup src/index.ts --clean --dts --sourcemap --format cjs --target node12 --minify-whitespace",
    "build:docs": "typedoc --name SnowTransfer --excludeExternals --sort static-first --sort alphabetical"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DasWolke/SnowTransfer.git"
  },
  "bugs": "https://github.com/DasWolke/SnowTransfer/issues",
  "author": "wolke <wolke@weeb.sh>",
  "license": "MIT",
  "dependencies": {
    "discord-api-types": "^0.37.50",
    "form-data": "^4.0.0",
    "undici": "^5.22.1"
  },
  "devDependencies": {
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "tsup": "^7.1.0",
    "typedoc": "^0.24.8",
    "typedoc-plugin-mdn-links": "^3.0.3",
    "typescript": "^5.1.6"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ]
}
